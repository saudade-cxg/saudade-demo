    "@ant-design/pro-layout": "^6.4.19",
    "@antv/data-set": "^0.11.7",
    "@antv/g2": "^4.0.15",
    "@antv/g6": "^3.8.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.161",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.22",
    "classnames": "^2.2.6",
    "lint-staged": "^10.0.7",
    "loadsh": "^0.0.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-guide": "^1.2.9",
    "react-window": "^1.8.5",
    "umi": "^3.2.22",
    "xlsx": "^0.16.8",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@ant-design/pro-layout": "^6.4.19"
